{
  "name": "@thomastrepanier/demo-library",
  "version": "2.0.2",
  "scripts": {
    "ng": "ng",
    "build-dev": "ng build demo-library --configuration=development",
    "build-prod": "ng build demo-library --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test demo-library",
    "test-ci": "ng test demo-library --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "tslint --project .",
    "publish": "ng build demo-library && cd ../../dist/demo-library && npm publish"
  },
  "peerDependencies": {
    "@angular/common": "^16.0.0",
    "@angular/core": "^16.0.0",
    "lodash": "^4.17.21"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThomasTrepanier/log6307e-demo-library.git"
  },
  "publishConfig": {
    "@thomastrepanier:registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "tslint": "^6.1.3"
  }
}
